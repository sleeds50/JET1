@article{kaurWomenJewelryTraditional2012a,
  title = {Women and {{Jewelry}} -the {{Traditional}} and {{Religious Dimensions}} of {{Ornamentation}}},
  author = {Kaur, Prabhjot and Joseph, Ruby},
  date = {2012-01-01},
  journaltitle = {Coherence},
  shortjournal = {Coherence},
  volume = {3},
  pages = {39--49},
  abstract = {Personal adornment of jewelry besides enhancing beauty symbolize status, wealth and assets, excite the sense of prized possession ,titillate aesthetic satisfaction, display the craftsmanship that go into their creation and continue an age old tradition .These primitive forms of jewelry are a symbol of protective powers and traditional and religious affiliations. Indian culture differs from Western culture in the shared importance placed on the spiritual and ethnic values that permeate Indian life and imbue symbolic meaning into objects. Jewelry carries spiritual, ethnic meaning that underlies its use as a ritual artifact as the bride crosses the threshold of the extended family. This study examined the ritualistic use of jewelry during various occasions to develop insights into the Indian's perspective of possession meaning. As jewelry is lent or given to each new bride that enters the extended family, the jewelry grows heavier with nuanced meaning that becomes part of the legacy of the jewelry. Gifted and borrowed jewelry plays an indispensable role in the ritual purification and transformation of the bride from an 'other' commoditized female into part of the bridegroom's extended self and subsequent incorporation into the sacred domain of the extended family. Every traditional jewelry worn has sacred and ethnic meanings based in notions of auspiciousness that persist, even if a negative, indexical meaning profanes the possession. The Religious jewelry that is selected reflects one's consciousness and awareness at that moment and one's request for protection and blessings from the vast pantheon of Gods and Goddesses in their religion. Indian consumers seem comfortable with the temporal coexistence of sacred and profane meanings.},
  file = {G\:\\My Drive\\zotero\\Kaur and Joseph - 2012 - Women and Jewelry -the Traditional and Religious D.pdf}
}

